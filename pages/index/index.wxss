/**index.wxss**/
@import '/utils/css/demonStyle.wxss';

.index_load{
  position:absolute;
  height:100%;
  width:100%;
}

page{
  background-color: #1a523f;
}
.demon_tab {
  background-color: #fefefe;
  width: 690rpx;
  margin: 0 auto;
  /*border: 1rpx solid #ccc;*/
  border-radius: 5rpx;
  /*box-shadow: 0 0 10rpx #ccc;*/
}

.demon_tab_content_item {
  padding: 0 30rpx;
}
.rankbox{
  margin-top: 90rpx;
}
/*群排行个人样式*/
.rankbox .demon_userInfo_content{
  width: 460rpx;
  height:222rpx;
  border-radius: 4rpx;
}
.rankbox .demon_userInfo_content_item .text{
  display: block;
  color: #ffffff;
  font: 32rpx 'PingFang';
}
.rankbox .demon_userInfo_content_item .text:nth-child(2){
  margin-top: 20rpx;
}

.dynamicgroup{
  position: relative;
}
.dynamicgroup .nomsgBox{
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -200rpx;
  margin-top: -170rpx;
}
.dynamicgroup .nomsgimg{
  display: block;
  height: 240rpx;
  width: 400rpx;
  margin-bottom: 20rpx;
}
.dynamicgroup .nomsgtext{
  text-align: center;
  font: 30rpx 'PingFange SC';
  color: #DDDDDD;
}