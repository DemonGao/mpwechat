/* userInfo.wxss */
@import '/utils/css/demonStyle.wxss';

.demon_userInfo_content{
  width: 460rpx;
  border-radius: 4rpx;
  color: #ffffff;
  font: 32rpx 'PingFang';
}

.demon_userInfo_content_item view{
  display: block;
  vertical-align: text-top;
}
.demon_userInfo_content_item text:nth-child(2){
  margin-top: 20rpx;
}
.page-section-spacing{
  margin-top: 60rpx;
}
.scroll-view_H{
  white-space: nowrap;
}
.scroll-view-item{
  height: 300rpx;
}
.scroll-view-item_H{
  display: inline-block;
  width: 100%;
  height: 300rpx;
}
